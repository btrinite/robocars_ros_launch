<launch>
  <node pkg="robocars_steering_ctrl" type="robocars_steering_ctrl" ns="steering_ctrl" name="robocars_steering_ctrl">
     <remap from="/steering_ctrl/full" to="/virtual_steering_ctrl/full"/>
